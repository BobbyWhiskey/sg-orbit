{
    "version": 2,
    "name": "orbit",
    "public": true,
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "built-storybooks"
            }
        }
    ],
    "build": {
        "env": {
            "DOTENV_DISPLAY_WARNING": "none",
            "STORYBOOK_EXAMPLE_APP": "true"
        }
    },
    "routes": [{ "src": "/(.*)", "dest": "/built-storybooks/$1" }]
}
