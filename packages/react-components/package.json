{
  "name": "@sharegate/react-components",
  "version": "0.1.0",
  "main": "dist/index.js",
  "repository": "https://github.com/gsoft-inc/sg-brand.git",
  "author": "Groupe Sharegate inc.",
  "license": "Apache-2.0",
  "scripts": {
    "start": "run-p watch sb:start",
    "prebuild": "yarn clean",
    "build": "run-p build:*",
    "build:this": "babel src -d dist --config-file ./babel.config.js",
    "build:components": "lerna exec --scope @sharegate/react-* --ignore @sharegate/react-components --parallel -- babel src -d dist --config-file ../../babel.config.js --copy-files",
    "prewatch": "yarn clean",
    "watch": "run-p \"build:* --watch\"",
    "clean": "run-p clean:*",
    "clean:this": "rimraf dist",
    "clean:components": "lerna exec --scope @sharegate/react-* --ignore @sharegate/react-components --parallel -- rimraf dist",
    "sb:start": "start-storybook -c storybook",
    "sb:build": "build-storybook -c storybook -o storybook/dist"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "@sharegate/react-date-range-picker": "0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-transform-modules-commonjs": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@storybook/react": "5.1.3",
    "@svgr/webpack": "4.3.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "0.3.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "styled-jsx": "3.2.1"
  }
}
