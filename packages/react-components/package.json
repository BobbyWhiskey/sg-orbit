{
    "name": "@sharegate/react-components",
    "version": "0.3.1",
    "repository": "https://github.com/gsoft-inc/sg-brand.git",
    "author": "Groupe Sharegate inc.",
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "start": "yarn watch",
        "prebuild": "yarn clean",
        "build": "run-p build:*",
        "build:this": "babel src -d dist --config-file ./babel.config.js",
        "build:components": "lerna exec --scope @sharegate/react-* --ignore @sharegate/react-components --parallel -- babel src -d dist --config-file ../../babel.config.js --copy-files",
        "prewatch": "yarn clean",
        "watch": "run-p \"build:* --watch\"",
        "test:ci": "yarn chromatic:ci",
        "clean": "run-p clean:*",
        "clean:this": "rimraf dist",
        "clean:components": "lerna exec --scope @sharegate/react-* --ignore @sharegate/react-components --parallel -- rimraf dist",
        "prepublishOnly": "cross-env NODE_ENV=production && yarn build",
        "sb:start": "start-storybook -c storybook",
        "sb:build": "build-storybook -c storybook -o storybook/dist",
        "sb:deploy": "netlify deploy --prod --dir=storybook/dist --site=4b420380-aed1-4dc6-b002-6efe7b413025 --open",
        "sb:chromatic": "yarn sb:start -p 1135 --ci",
        "chromatic:test": "chromatic test --script-name=sb:chromatic",
        "chromatic:ci": "yarn chromatic:test --no-interactive"
    },
    "peerDependencies": {
        "@sharegate/css-normalize": "*",
        "@sharegate/semantic-ui-theme": "*",
        "@sharegate/tachyons": "*",
        "react": "*",
        "react-dom": "*",
        "semantic-ui-react": "*"
    },
    "dependencies": {
        "@babel/runtime-corejs3": "7.5.5",
        "@sharegate/react-components-shared": "0.2.1",
